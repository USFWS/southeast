{
  "name": "priorities.js",
  "version": "1.0.0",
  "description": "Script for the priorities webpage",
  "main": "index.js",
  "browserify-shim": {
    ".src/js/imageComparison.js": "ImageComparison"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "browserify": "browserify index.js | uglifyjs -mc > ../../../site/static/js/priorities.js",
    "watchify": "watchify index.js -o ../../../site/static/js/priorities.js -dv",
    "inspect": "browserify --full-paths index.js | uglifyjs -mc | Discify --open",
    "start": "npm run watchify",
    "build": "npm run browserify"
  },
  "author": "Roy Hewitt, @r0yhewitt",
  "license": "SAX-PD",
  "dependencies": {
    "events": "^1.1.1",
    "fws-scrollnav": "^1.13.0",
    "image-comparison": "^1.1.2",
    "is-dom": "^1.0.9",
    "lodash.debounce": "^4.0.8",
    "lodash.defaults": "^4.2.0",
    "lodash.filter": "^4.6.0",
    "lodash.foreach": "^4.5.0",
    "lodash.isarray": "^4.0.0",
    "parallaxify": "^0.1.1",
    "scrollreveal": "^3.3.5",
    "underscore.string": "^3.3.5",
    "zenscroll": "^3.3.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "browserify-shim": "^3.8.14",
    "disc": "^1.3.2",
    "uglify-es": "^3.3.9",
    "watchify": "^3.9.0"
  }
}
